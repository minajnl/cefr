INSERT INTO cefr VALUES('subjective','C1','adjective','[]','[''/səbˈdʒek.tɪv/'']','[''Dictionary example: a subjective judgment '', "Learner example:I really love my city and country, but I am afraid I''m a little bit subjective. (Certificate in Advanced English; C1; Hungarian)"]');
INSERT INTO cefr VALUES('simply','B2','adverb','[]','[''/ˈsɪm.pli/'']','["Dictionary examples:We simply don''t have the time.The hunger in parts of Africa is terrible - there''s simply no other word for it.", "Learner example:That''s simply unbelievable! (First Certificate in English; B2; Italian)"]');
