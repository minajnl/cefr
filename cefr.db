INSERT INTO cefr VALUES('grade','C1','verb','[]','[''/greɪd/'']','[''Dictionary example:The fruit is washed and then graded by size.'', ''Learner example:Our members graded everything on a scale from 1 to 5, with 1 being very poor, and 5 being excellent. (Certificate in Advanced English; C1; Swedish)'']');
