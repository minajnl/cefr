INSERT INTO cefr VALUES('let','A2','verb','[]','[''/let/'']','["Dictionary examples:After the film, let''s go out to dinner.Let''s not argue.", "Learner example:Let''s go shopping. (Key English Test; A2; Japanese)"]');
